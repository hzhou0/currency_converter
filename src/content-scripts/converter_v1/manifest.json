{
    "name": "obj ext",
    "description": "my ext",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./object/icon16.png",
        "32": "./object/icon32.png",
        "48": "./object/icon48.png",
        "128": "./object/icon128.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
        //"https://www.google.ca/*"
    ]
}